<h1>Sessions#new</h1>
<%= form_tag do %>
  <div>
    <%= label_tag :email %>
    <%= text_field_tag :email %>
  </div>

  <div>
    <%= label_tag :password %>
    <%= password_field_tag :password %>
  </div>

  <div>
    <%= submit_tag "Sign In" %> or <%= link_to "Sign Up", sign_up_path %>
  </div>

<% end %>
